<% unless current_user? %>
<h1>Login with Reddit to view your Reddit feed</h1>
<%= link_to "Login with Reddit", "https://www.reddit.com/api/v1/authorize?client_id=#{ENV['REDDIT_KEY']}&response_type=code&state=RANDOM_STRING&redirect_uri=#{ENV['REDIRECT_URI']}&duration=permanent&scope=identity", class: "button" %>
<% else %>
<h1><%= current_user.username %>'s Subreddits</h1>
<% end %>
